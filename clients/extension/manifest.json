{
  "name": "__MSG_appName__",
  "version": "5.0.0",
  "minimum_chrome_version": "76",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/lh_logo_bg.png",
    "128": "images/lh_logo_bg.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/extension-entry.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "38": "images/lh_logo_icon.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'none'"
}
